<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.im.mapper.AdverMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.im.entity.Adver">
        <id column="id" property="id" />
           <result column="url" property="url" />
           <result column="type" property="type" />
           <result column="jump_url" property="jumpUrl" />
           <result column="timeout" property="timeout" />
           <result column="is_on" property="isOn" />
           <result column="ts_create" property="tsCreate" />
       </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, url, type, jump_url, timeout, is_on, ts_create
    </sql>
    <select id="pagination" resultType="org.jeecg.modules.im.entity.Adver">
        SELECT <include refid="Base_Column_List" /> from im_adver
    </select>

</mapper>
