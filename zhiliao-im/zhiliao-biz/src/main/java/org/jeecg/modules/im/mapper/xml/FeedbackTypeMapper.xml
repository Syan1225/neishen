<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.im.mapper.FeedbackTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jeecg.modules.im.entity.FeedbackType">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="message" property="message" />
        <result column="ts_create" property="tsCreate" />
        <result column="order_no" property="orderNo" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, message, ts_create, order_no
    </sql>

    <select id="findAll" resultMap="BaseResultMap">
        select * from im_feedback_type
    </select>

</mapper>
