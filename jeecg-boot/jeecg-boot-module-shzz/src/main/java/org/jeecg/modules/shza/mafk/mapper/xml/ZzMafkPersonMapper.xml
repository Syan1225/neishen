<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.shza.mafk.mapper.ZzMafkPersonMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  zz_mafk_person
		WHERE
			 zz_mafk_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" resultType="org.jeecg.modules.shza.mafk.entity.ZzMafkPerson">
		SELECT * 
		FROM  zz_mafk_person
		WHERE
			 zz_mafk_id = #{mainId}
			 and type = #{type}	</select>
</mapper>
