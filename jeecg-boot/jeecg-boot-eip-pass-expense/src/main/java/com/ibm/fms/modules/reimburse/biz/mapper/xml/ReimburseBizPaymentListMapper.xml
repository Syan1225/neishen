<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ibm.fms.modules.reimburse.biz.mapper.ReimburseBizPaymentListMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  reimburse_biz_payment_list 
		WHERE
			 apply_no = #{mainId} 
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="com.ibm.fms.modules.reimburse.biz.entity.ReimburseBizPaymentList">
		SELECT * 
		FROM  reimburse_biz_payment_list
		WHERE
			 apply_no = #{mainId} 
	</select>
</mapper>
