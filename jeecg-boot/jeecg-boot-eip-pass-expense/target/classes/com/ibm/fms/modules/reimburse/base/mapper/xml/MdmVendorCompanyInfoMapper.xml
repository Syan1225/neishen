<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ibm.fms.modules.reimburse.base.mapper.MdmVendorCompanyInfoMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  mdm_vendor_company_info 
		WHERE
			 vendor_id = #{mainId} 
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="com.ibm.fms.modules.reimburse.base.entity.MdmVendorCompanyInfo">
		SELECT * 
		FROM  mdm_vendor_company_info
		WHERE
			 vendor_id = #{mainId} 
	</select>
</mapper>
